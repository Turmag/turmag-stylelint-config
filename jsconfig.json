{
    "compilerOptions": {
        "target": "ESNext",
        "module": "ESNext",
        "moduleResolution": "Node",
        "resolveJsonModule": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "noImplicitAny": true,
        "strictPropertyInitialization": true,
        "noImplicitThis": true,
        "skipLibCheck": true
    },
    "include": ["eslint.config.js"],
    "exclude": ["node_modules"]
}
